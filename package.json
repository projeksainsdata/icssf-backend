{
  "name": "icssfitera2024.api",
  "version": "1.0.0",
  "description": "API for ICSS Fitera 2024",
  "main": "src/app.js",
  "author": "EXSTR",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "scripts": {
    "clean": "mkdir build",
    "build-server": "babel --out-dir ./build . --source-maps --copy-files --ignore 'node_modules/**/*.js'",
    "build": "npm run clean && npm run build-server",
    "start": "pm2 start ./build/app.js -i ${NODE_PROCESSES} --no-daemon",
    "dev": "nodemon src/app.js",
    "lint": "./node_modules/.bin/eslint --ignore-path .gitignore . --fix",
    "create-superuser": "node bin/create-superuser.js",
    "test": "cross-env NODE_ENV=test jest --testTimeout=2000"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "type": "module",
  "dependencies": {
    "@babel/cli": "^7.12.17",
    "@babel/core": "^7.12.17",
    "@babel/node": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@babel/runtime": "^7.12.18",
    "@godaddy/terminus": "^4.6.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^8.5.1",
    "memory-cache": "^0.2.0",
    "mongoose": "^6.8.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.14",
    "rate-limiter-flexible": "^5.0.3",
    "web-push": "^3.6.7",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.3",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-import": "^2.26.0",
    "faker": "^5.5.3",
    "husky": "^8.0.2",
    "jest": "^29.7.0",
    "nodemon": "^2.0.20",
    "pm2": "^5.2.2",
    "prettier": "^2.8.1",
    "supertest": "^7.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3"
  }
}
